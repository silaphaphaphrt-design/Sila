#include <stdio.h>

int GetSet(int **d, int *number);

int main() {
    int *data, num;

    GetSet(&data, &num);
    return 0;
}

int GetSet(int **d, int *number) {

    printf("Enter number: ");
    scanf("%d", number);

    for (int i = 0; i < *number; i++) {
        printf("[%d] ", i + 1);
        scanf("%d", &(*d)[i]);
    }

    return 0;
}
-------------------------------------------------
Enter number: 5
[1] 2
[2] 3
[3] 4
[4] 2
[5] 22
