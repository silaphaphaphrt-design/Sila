#include <stdio.h>

void swapVal(int *x, int *y);

int main()
{
    int a, b;

    scanf("%d", &a);
    scanf("%d", &b);

    printf("\n--- SWAPPING REPORT ---\n");
    printf("Before Swap - Num1: %d, Num2: %d\n", a, b);

    swapVal(&a, &b);

    printf("After Swap - Num1: %d, Num2: %d\n", a, b);

    return 0;
}

void swapVal(int *x, int *y)
{
    int tmp;
    tmp = *x;
    *x = *y;
    *y = tmp;
}
