#include <stdio.h>

#define N 4

struct Student {
    int id;
};

int findID(struct Student s[], int n, int key)
{
    int i;
    for(i = 0; i < n; i++)
    {
        if(s[i].id == key)
            return 1;
    }
    return 0;
}

int main()
{
    struct Student list[N];
    int search;
    int status;
    int i;

    for(i = 0; i < N; i++)
    scanf("%d", &list[i].id);

    scanf("%d", &search);

    printf("--- ID SEARCH REPORT ---\n");
    printf("Recorded IDs:");
    for(i = 0; i < N; i++)
    printf(" %d", list[i].id);

    printf("\n");
    printf("Search ID: %d\n", search);

    status = findID(list, N, search);

    if(status)
        printf("ID FOUND in the system.\n");
    else
        printf("ID NOT FOUND in the system.\n");

    return 0;
}
