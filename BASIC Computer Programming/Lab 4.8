#include <stdio.h>

int main() {
    int val[10];    
    int check[10]; 
    int n, i, j, cnt;

    for (i = 0; i < 10; i++) check[i] = 0;


    if (scanf("%d", &n) != 1) return 0;
    if (n > 10 || n < 1) n = 10;


    for (i = 0; i < n; i++) {
        scanf("%d", &val[i]);
    }

    printf("\n--- FREQUENCY ANALYSIS REPORT ---\n");
    printf("Total elements recorded (N): %d\n", n);
    
    printf("Recorded Numbers: ");
    for (i = 0; i < n; i++) {
        printf("%d%s", val[i], (i == n - 1) ? "" : " ");
    }
    printf("\n");

    printf("--- FREQUENCY TABLE ---\n");
    printf("Number | Frequency\n");
    printf("-------|----------\n");

    for (i = 0; i < n; i++) {
        if (check[i] == 0) {
            cnt = 1;
            for (j = i + 1; j < n; j++) {
                if (val[i] == val[j]) {
                    cnt++;
                    check[j] = -1; 
                }
            }
            
            printf("%-6d | %d\n", val[i], cnt);
        }
    }

    return 0;
}
