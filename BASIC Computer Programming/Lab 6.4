#include <stdio.h>

void calcSumProd(int a, int b, int *sum, int *prod);

int main()
{
    int a, b;
    int sum, prod;

    scanf("%d", &a);
    scanf("%d", &b);

    calcSumProd(a, b, &sum, &prod);

    printf("\n--- CALCULATION REPORT ---\n");
    printf("Input Values: A = %d, B = %d\n", a, b);
    printf("Result Sum(A+B): %d\n", sum);
    printf("Result Product(A*B): %d\n", prod);

    return 0;
}

void calcSumProd(int a, int b, int *sum, int *prod)
{
    *sum = a + b;
    *prod = a * b;
}
