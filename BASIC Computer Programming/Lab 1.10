#include <stdio.h>

int main() {
    int clearanceLevel, age, isActive;

    if (scanf("%d %d %d", &clearanceLevel, &age, &isActive) != 3) {
        return 1;
    }

    if ((clearanceLevel == 3 && isActive == 1) ||
        (clearanceLevel == 2 && age >= 25 && isActive == 1)) {

        printf("Access Level : %d\n", clearanceLevel);
        printf("Age : %d\n", age);
        printf("Active : %d\n", isActive);
        printf("Access Granted");
    } else {
       printf("Access Level : %d\n", clearanceLevel);
        printf("Age : %d\n", age);
        printf("Active : %d\n", isActive);
        printf("Access Denied");
    }

    return 0;
}
