#include <stdio.h>
#define R 2
#define C 3


void transpose(int src[R][C], int dest[C][R]) {
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            dest[j][i] = src[i][j];
        }
    }
}

void multiply(int m1[R][C], int m2[C][R], int res[R][R]) {
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < R; j++) {
            res[i][j] = 0;
            for (int k = 0; k < C; k++) {
                res[i][j] += m1[i][k] * m2[k][j];
            }
        }
    }
}

void show(int rows, int cols, int *mat) {
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {

            printf("%d%s", *(mat + i * cols + j), (j == cols - 1) ? "" : " ");
        }
        printf("\n");
    }
}

int main() {
    int A[R][C], B[R][C], BT[C][R], Res[R][R];


    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            scanf("%d", &A[i][j]);
        }
    }


    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            scanf("%d", &B[i][j]);
        }
    }

  
    transpose(B, BT);
    multiply(A, BT, Res);

  
    printf("--- MATRIX MULTIPLICATION REPORT ---\n\n");

    printf("Matrix A (2x3):\n");
    show(R, C, (int *)A);
    printf("\n");

    printf("Matrix B (2x3):\n");
    show(R, C, (int *)B);
    printf("\n");

    printf("Matrix B Transposed (BT, 3x2):\n");
    show(C, R, (int *)BT);
    printf("\n");

    printf("Result Matrix C (A x BT, 2x2):\n");
    show(R, R, (int *)Res);

    return 0;
}
