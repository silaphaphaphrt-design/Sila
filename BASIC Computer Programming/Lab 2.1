#include <stdio.h> 

int main() {
    int N, i;

    if (scanf("%d", &N) == 1 && N > 0) {
        printf("Input: %d\n", N);
        for (i = 0; i < N; i++) {
        printf("Hello Loop!\n");
    }
   } else {
      printf("Input: %d\n", N);
      printf("Error\n");
    }
    return 0;
}
