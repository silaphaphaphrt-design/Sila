#include <stdio.h>
#define R 3 
#define C 3 

int main() {
    int mat[R][C];  
    int i, j;


    for (i = 0; i < R; i++) {
        for (j = 0; j < C; j++) {
            scanf("%d", &mat[i][j]);
        }
    }

    printf("\n--- TWO-DIMENSIONAL ARRAY REPORT ---\n");
    printf("Array Size: %d Rows x %d Columns\n", R, C);
    printf("Content (Table Format):\n");

    for (i = 0; i < R; i++) {
        for (j = 0; j < C; j++) {
      
            printf("%d%s", mat[i][j], (j == C - 1) ? "" : " ");
        }
        printf("\n");
    }

    return 0;
}
