#include <stdio.h>

int main() {
    int n, target, i, count = 0;
    int data[10];
    int max = 10;


    if (scanf("%d", &n) != 1) return 0;
    if (n > max) n = max;
    if (n < 0) n = 0;

    for (i = 0; i < n; i++) {
        scanf("%d", &data[i]);
    }

    scanf("%d", &target);

    for (i = 0; i < n; i++) {
        if (data[i] == target) {
            count++;
        }
    }


    printf("--- FREQUENCY ANALYSIS REPORT ---\n");
    printf("Total elements recorded (N): %d\n", n);
    printf("Recorded Numbers:");
    for (i = 0; i < n; i++) {
        printf(" %d", data[i]);
    }
    printf("\nSearch Value: %d\n", target);
    printf("Frequency Count: %d\n", count);

    return 0;
}
