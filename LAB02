#include <stdio.h>

void GetSet(int **ptrarr, int *number);

int main(){
    int *arr, n;

    printf("Plsese Fill number ");

    scanf("%d", &n);

    GetSet(&arr, &n);

    return 0;
}

void GetSet(int **ptrarr, int *number){
    for (int i = 0; i < *number; i++){
        
        printf("[%d] ", i + 1);

        scanf("%d", &(*ptrarr)[i]);
    }
}
-----------------------------------------------
Plsese Fill number 5
[1] 3
[2] 4
[3] 5
[4] 6
[5] 7
